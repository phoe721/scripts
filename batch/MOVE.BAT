@ECHO OFF
SETLOCAL ENABLEDELAYEDEXPANSION

SET /P FILE_PATH=SPECIFY THE FILE PATH: 
SET /P IMG_PATH=SPECIFY THE IMAGE PATH TO LOOK FOR: 
SET /P NEW_IMG_PATH=SPECIFY THE IMAGE PATH TO MOVE TO: 

FOR /F "TOKENS=*" %%A IN (%FILE_PATH%) DO (
	SET IMAGE_FILE=%IMG_PATH%\%%A
	IF EXIST !IMAGE_FILE! (
		REM ECHO FOUND !IMAGE_FILE!
		MOVE /Y !IMAGE_FILE! %NEW_IMG_PATH%
	) ELSE (
		ECHO NOT FOUND !IMAGE_FILE! >> NOT_FOUND.TXT
	)
)
